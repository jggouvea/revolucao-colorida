    <text:span text:style-name="T309">O</text:span><text:span text:style-name="T310">
    </text:span><text:span text:style-name="T311">Estado</text:span><text:span text:style-name="T312">
    </text:span><text:span text:style-name="T313">de</text:span><text:span text:style-name="T314">
